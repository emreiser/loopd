<h2>Manage feeds</h2>
<div id='add-category'>
	<%= render partial: 'new_category_form' %>
</div>

<h3>All feeds</h3>
<ul id='all-feeds'>
	<% @feeds.each do |feed| %>
		<li><%= link_to feed.url %></li>

	<% end %>
</ul>

<h3>Categories</h3>
<ul id='all-categories'>
	<% @categories.each do |category| %>
		<li><%= category.name %></li>

	<% end %>
</ul>